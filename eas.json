{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "android": { "gradleCommand": ":app:bundleRelease" },
      "ios": { "simulator": false }
    },
    "staging": {
      "developmentClient": false
    },
    "preview": {
      "developmentClient": false
    }
  },
  "submit": {
    "production": {
      "ios": { "appleId": "you@example.com" },
      "android": {}
    }
  },
  "update": {
    "production": {
      "channel": "production",
      "env": { "EXPO_PUBLIC_ENV": "production" }
    },
    "staging": {
      "channel": "staging",
      "env": { "EXPO_PUBLIC_ENV": "staging" }
    },
    "preview": {
      "channel": "sandbox",
      "env": { "EXPO_PUBLIC_ENV": "sandbox" }
    }
  }
}
