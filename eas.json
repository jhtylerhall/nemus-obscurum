{
  "cli": {
    "version": ">= 8.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "environment": "production",
      "node": "20.11.1"
    },
    "production": {
      "extends": "base",
      "channel": "production",
      "autoIncrement": true,
      "distribution": "store"
    },
    "staging": {
      "extends": "base",
      "channel": "staging",
      "distribution": "internal"
    },
    "sandbox": {
      "extends": "base",
      "channel": "sandbox",
      "distribution": "internal",
      "developmentClient": false
    }
  },
  "submit": {
    "production": {}
  }
}
